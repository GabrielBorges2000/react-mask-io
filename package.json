{
  "name": "react-mask-io",
  "description": "Masked input component for React",
  "version": "1.0.11",
  "homepage": "https://github.com/GabrielBorges2000/react-mask-io",
  "license": "MIT",
  "author": "Gabriel Borges <gabriel.vscode@gmail.com>",
  "keywords": [
    "react",
    "input",
    "mask",
    "masked",
    "react-component",
    "react-mask",
    "react-mask-io",
    "react-input-mask"
  ],
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "generate-package": "npm version patch && npm run build && npm pack",
    "lint": "eslint --fix ./src/**/*.{ts,tsx} ",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "commit": "git-cz --hook",
    "prepare": "husky",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "prepublishOnly": "npm run clean && npm run build",
    "publish:patch": "npm run version:patch && npm publish --access public",
    "publish:minor": "npm run version:minor && npm publish --access public",
    "publish:major": "npm run version:major && npm publish --access public",
    "publish:alpha": "npm run version:patch && npm publish --tag alpha --access public",
    "publish:beta": "npm run version:patch && npm publish --tag beta --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GabrielBorges2000/react-mask-io.git"
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "prop-types": "^15.7.2",
    "warning": "^4.0.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@types/node": "^24.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "chai": "^4.2.0",
    "console-polyfill": "^0.3.0",
    "cross-env": "^5.2.1",
    "eslint": "^9.39.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "git-cz": "^4.9.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^4.0.2",
    "lint-staged": "^9.5.0",
    "mocha": "^6.2.2",
    "prettier": "^3.5.3",
    "puppeteer": "^1.20.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.20.0"
  }
}
